{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDirs": [
      "./src",
      "./prisma",
      "./test"
    ],
    "sourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}